{
  "name": "@nextcloud/password-confirmation",
  "version": "2.0.0",
  "description": "Promise-based password confirmation wrapper for Nextcloud",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "babel ./lib --out-dir dist --extensions '.ts,.tsx' --source-maps && tsc --emitDeclarationOnly",
    "build:doc": "typedoc --out dist/doc lib && touch dist/doc/.nojekyll",
    "check-types": "tsc",
    "dev": "babel ./lib --out-dir dist --extensions '.ts,.tsx' --watch",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "nextcloud"
  ],
  "homepage": "https://github.com/nextcloud/nextcloud-password-confirmation#readme",
  "author": "Christoph Wurst",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nextcloud/nextcloud-password-confirmation"
  },
  "bugs": {
    "url": "https://github.com/nextcloud/nextcloud-password-confirmation/issues"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "7.18.2",
    "@babel/plugin-transform-arrow-functions": "7.17.12",
    "@babel/preset-env": "7.18.2",
    "@babel/preset-typescript": "^7.9.0",
    "@nextcloud/browserslist-config": "^2.0.0",
    "@nextcloud/typings": "^1.2.1",
    "babel-loader": "8.2.5",
    "typedoc": "^0.22.7",
    "typescript": "^4.0.2"
  },
  "dependencies": {
    "core-js": "^3.6.4"
  },
  "browserslist": [
    "extends @nextcloud/browserslist-config"
  ]
}
